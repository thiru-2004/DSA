def Search_number(list,element):
    position=0
    while True:
        if list[position]==element:
            return position
        position+=1
        if position==len(list):
            return -1

list=[1,2,3,4,5,6,7,8,9,]
element=int(input("enter the number to find"))
result=Search_number(list,element)
if(list[result]==element):
    print("element" ,element,"is found at",result,"position")
else:
    print("element not found")
